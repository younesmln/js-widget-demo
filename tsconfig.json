{
    "compilerOptions": {
      "target": "es5",
      // "module": "esnext",
      "jsx": "react",
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports":true,
      "lib": ["es5", "es6", "dom"]
    },
  }